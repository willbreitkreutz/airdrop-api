  "license": "MIT",
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "sqlite3": "^5.1.6",
    "ws": "^8.13.0"
  }
}
